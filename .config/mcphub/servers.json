{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "env": {
        "ALLOWED_DIRECTORY": "."
      },
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "${ALLOWED_DIRECTORY}"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "context7": {
      "command": "npx",
      "env": {
        "DEFAULT_MINIMUM_TOKENS": "5000"
      },
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    }
  },
  "nativeMCPServers": [
    
  ]
}